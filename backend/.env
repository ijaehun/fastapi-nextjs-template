# backend/.env
# 기본 설정
APP_NAME=My Web Template
DEBUG=True
ENVIRONMENT=development

# 서버 설정  
HOST=0.0.0.0
PORT=8000

# 보안 (실제 배포시 변경 필수!)
SECRET_KEY=change-this-secret-key-in-production

# 데이터베이스 (필요시 설정)
DATABASE_URL=sqlite:///./app.db

# 외부 서비스 API 키들 (필요시 추가)
# OPENAI_API_KEY=your-openai-key
# KAKAO_API_KEY=your-kakao-key